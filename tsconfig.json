{
  "compilerOptions": {
    "target": "es5",
    "lib": [ "dom" ],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "noFallthroughCasesInSwitch": true,
    "sourceMap": true,
    "outDir": "./public/",
  },
  "include": [
    "src",
],
  "exclude": [
    "node_modules",
    "/**/*.d.ts"
  ]
}
